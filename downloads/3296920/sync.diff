 //----------------------------------------------------------------------
+// read thread functions
+//----------------------------------------------------------------------
+void* Node::_runReadThread( ConnectionPtr connection )
+{
+    ReadThread::Request request;
+    request.handled = true;
+
+    if( connection->isConnected( ))
+    {
+        request.connection = connection;
+
+        while( connection->isConnected( ))
+        {
+            uint64_t size = 0;
+            connection->recvNB( &size, sizeof( size ));
+            if( !connection->recvSync( 0, 0 ))
+                continue;
+
+            EQASSERT( size > sizeof( size ));
+            _nodes.lock.set();
+            Command& command = _commandCache.alloc( connection, 0, this, size );
+            command.retain();
+            _nodes.lock.unset();
+            uint8_t* ptr = reinterpret_cast< uint8_t* >( command.getPacket()) +
+                               sizeof( uint64_t );
+
+            connection->recvNB( ptr, size - sizeof( uint64_t ));
+            const bool gotData = connection->recvSync( 0, 0 );    
+
+            EQASSERT( gotData );
+            EQASSERT( command.isValid( ));
+
+            if( !gotData )
+            {
+                EQERROR << "Incomplete packet read: " << command << std::endl;
+                command.release();
+                continue;
+            }
+            
+            request.handled.waitEQ( true );
+            request.handled = false;
+            request.command = &command;
+            _requests.push( &request );
+            _incoming.interrupt();
+        }
+    }
+    else if( connection->isListening( ))
+    {
+        request.size = 0;
+        while( connection->isListening( ))
+        {
+            connection->acceptNB();
+            ConnectionPtr newConn = connection->acceptSync();
+            if( !newConn )
+                continue;
+
+            request.handled.waitEQ( true );
+            request.handled = false;
+            request.connection = newConn;
+            _requests.push( &request );
+            _incoming.interrupt();
+        }
+    }
+
+    request.connection = connection;
+    request.size = std::numeric_limits< uint64_t >::max();
+    request.handled = false;
+    _requests.push( &request );
+    _incoming.interrupt();
+    request.handled.waitEQ( true );
+    
+    request.connection = 0;
+    return EXIT_SUCCESS;
+}
